public class DescArticles {
  
  private Domain domain;
  
  private int total;
  
  private int currentPage;
  
  private List<DescArticle> values;
  
  public DescArticles(Domain domain) {
    this.domain = domain;
    currentPage = 1; 
  }

  public Domain getDomain() { return domain; }
  public void setDomain(Domain domain) { this.domain = domain; }

  public int getTotal() { return total; }
  public void setTotal(int total) { this.total = total; }

  public int getCurrentPage() { return currentPage; }
  public void setCurrentPage(int currentPage) { this.currentPage = currentPage; }

  public List<DescArticle> getValues() { return values; }
  public void setValues(List<DescArticle> values) { this.values = values; }
  
}
